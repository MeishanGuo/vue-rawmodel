{
  "name": "vue-contextable",
  "version": "1.11.1",
  "description": "Contextable.js plugin for Vue.js v2. Form validation has never been easier!",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
